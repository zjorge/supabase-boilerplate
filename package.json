{
  "name": "supabase-nextjs-boilerplate",
  "version": "1.0.0",
  "description": "Production-ready Supabase + Next.js boilerplate with RLS and OAuth",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --local > types/database.types.ts"
  },
  "dependencies": {
    "@heroui/react": "^2.6.10",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.45.7",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.468.0",
    "next": "14.2.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.21",
    "postcss": "^8.4.49",
    "supabase": "^1.207.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
